#main-container.container style="padding-top:20px; padding-bottom: 20px;"
  .row
    .col-md-12  
      ol.breadcrumb
        li
          a href="index.html"  Home
        li 
          = link_to "Dasboard", setting_path
        li.active Following Services
    br/
    = render "side_bar"
    .col-sm-9.col-md-9
      ul.nav.nav-tabs
        li.active
          a data-toggle="tab" href="#inbox" 
            | My Following Services
      .tab-content
        #services.active.tab-pane
          .events
            .row
              - @followees.each do |service|
                .col-sm-6
                  .media
                    a.pull-left href="#"
                      - if service.avatar.file == nil
                        img.media-object.thumbnail alt="100x120" data-src="holder.js/100x120" src="data:image/png;base64," style=("width: 100px; height: 120px;") /
                      - else
                        img.media-object.thumbnail alt="100x120" src="#{service.avatar.large_avatar}"
                    .media-body
                      h4.event-title
                        = link_to "#{service.title}", service_path(service)
                      small.text-muted
                        i.fa.fa-map-marker
                        = " Verified" if service.user.verify_user.is_verify == true
                        = " Not Verified" if service.user.verify_user.is_verify == false
                      p =h truncate(service.description, :length => 80)
                  /! media